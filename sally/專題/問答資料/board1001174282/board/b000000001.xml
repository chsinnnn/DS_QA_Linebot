  <content>&lt;p&gt;struct Node {&lt;br /&gt;&#xD;
&amp;nbsp;&amp;nbsp; &amp;nbsp;int num;&lt;br /&gt;&#xD;
&amp;nbsp;&amp;nbsp; &amp;nbsp;Node * left;&lt;br /&gt;&#xD;
&amp;nbsp;&amp;nbsp; &amp;nbsp;Node * right;&lt;br /&gt;&#xD;
}; // Node&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;void insertnode ( Node * &amp;amp;root, int N ) {&lt;br /&gt;&#xD;
&amp;nbsp;&amp;nbsp; &amp;nbsp;if ( root == NULL ) {&lt;br /&gt;&#xD;
&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp;root = new Node;&lt;br /&gt;&#xD;
&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp;root -&amp;gt; num = N;&lt;br /&gt;&#xD;
&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp;root -&amp;gt; left = NULL;&lt;br /&gt;&#xD;
&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp;root -&amp;gt; right = NULL;&lt;br /&gt;&#xD;
&amp;nbsp;&amp;nbsp; &amp;nbsp;} // if&lt;br /&gt;&#xD;
&amp;nbsp;&amp;nbsp; &amp;nbsp;else if ( N &amp;gt; root -&amp;gt; num )&amp;nbsp;&lt;br /&gt;&#xD;
&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp;insertnode ( root -&amp;gt; right, N );&lt;br /&gt;&#xD;
&amp;nbsp;&amp;nbsp; &amp;nbsp;else if ( N &amp;lt; root -&amp;gt; num )&lt;br /&gt;&#xD;
&amp;nbsp;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;insertnode ( root -&amp;gt; left, N );&amp;nbsp;&amp;nbsp; &amp;nbsp;&lt;br /&gt;&#xD;
} // insertnode&lt;/p&gt;&#xD;
&#xD;
