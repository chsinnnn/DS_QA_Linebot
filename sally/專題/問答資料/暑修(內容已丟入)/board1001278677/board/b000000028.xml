  <content>&lt;p&gt;110. &#x5BEB;&#x51FA;C++&#x7A0B;&#x5F0F;&#x78BC;&#x8AAA;&#x660E;&#x50B3;&#x503C;&#x547C;&#x53EB;call by value&#x3002;&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;&#x7B54;:&amp;nbsp;&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;int changeValue(int x) {&lt;br /&gt;&#xD;
&amp;nbsp; &amp;nbsp; x&amp;nbsp;= x / 10; // &#x5C07;&#x50B3;&#x5165;&#x7684;&#x53C3;&#x6578;&#x9032;&#x884C;&#x905E;&#x589E;&#x64CD;&#x4F5C;&lt;br /&gt;&#xD;
&amp;nbsp; &amp;nbsp; return x;&lt;br /&gt;&#xD;
}&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;int main() {&lt;br /&gt;&#xD;
&amp;nbsp; &amp;nbsp; int initial = 100;&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;&amp;nbsp; &amp;nbsp; std::cout &amp;lt;&amp;lt; &amp;quot;&#x547C;&#x53EB;&#x51FD;&#x6578;&#x524D;&#x7684;initial&#xFF1A; &amp;quot; &amp;lt;&amp;lt; initial &amp;lt;&amp;lt; std::endl;&lt;br /&gt;&#xD;
&amp;nbsp; &amp;nbsp; //&#x8F38;&#x51FA;: 100&lt;br /&gt;&#xD;
&amp;nbsp; &amp;nbsp;&amp;nbsp;&lt;br /&gt;&#xD;
&amp;nbsp; &amp;nbsp; int after = changeValue(initial);&amp;nbsp;&lt;br /&gt;&#xD;
&amp;nbsp; &amp;nbsp; // call by value&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;&amp;nbsp; &amp;nbsp; std::cout &amp;lt;&amp;lt; &amp;quot;&#x547C;&#x53EB;&#x51FD;&#x6578;&#x5F8C;&#x7684;initial&#xFF1A; &amp;quot; &amp;lt;&amp;lt; initial &amp;lt;&amp;lt; std::endl;&lt;br /&gt;&#xD;
&amp;nbsp; &amp;nbsp; //&#x8F38;&#x51FA;: 100&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;&amp;nbsp; &amp;nbsp; std::cout &amp;lt;&amp;lt; &amp;quot;&#x51FD;&#x6578;&#x8F38;&#x51FA;&#x7684;&#x503C;&#xFF1A; &amp;quot; &amp;lt;&amp;lt; after &amp;lt;&amp;lt; std::endl;&lt;br /&gt;&#xD;
&amp;nbsp; &amp;nbsp; //&#x8F38;&#x51FA;:10&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;&amp;nbsp; &amp;nbsp; return 0;&lt;br /&gt;&#xD;
}&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;&#x8F38;&#x51FA;&#x70BA;:&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;&#x547C;&#x53EB;&#x51FD;&#x6578;&#x524D;&#x7684;initial&#xFF1A; 100&lt;br /&gt;&#xD;
