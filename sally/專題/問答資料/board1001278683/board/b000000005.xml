  <content>&lt;p&gt;//&#x64B0;&#x5BEB;&#x7A0B;&#x5F0F;&#x78BC;&#x8AAA;&#x660E;&#x6C23;&#x6CE1;&#x6392;&#x5E8F;bubble sort&#x5982;&#x4F55;&#x63D0;&#x65E9;&#x7D50;&#x675F;&#x57F7;&#x884C;&#x3002;&lt;br /&gt;&#xD;
#include &amp;lt;iostream&amp;gt;&lt;br /&gt;&#xD;
using namespace std;&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;&lt;br /&gt;&#xD;
/* run this program using the console pauser or add your own getch, system(&amp;quot;pause&amp;quot;) or input loop */&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;int main(int argc, char** argv) {&lt;br /&gt;&#xD;
&amp;nbsp;&amp;nbsp; &amp;nbsp;int n;&lt;br /&gt;&#xD;
&amp;nbsp;&amp;nbsp; &amp;nbsp;int tp;&lt;br /&gt;&#xD;
&amp;nbsp;&amp;nbsp; &amp;nbsp;bool ischanged;&lt;br /&gt;&#xD;
&amp;nbsp;&amp;nbsp; &amp;nbsp;cout &amp;lt;&amp;lt; &amp;quot;input n:&amp;quot;;&lt;br /&gt;&#xD;
&amp;nbsp;&amp;nbsp; &amp;nbsp;cin &amp;gt;&amp;gt; n;&lt;br /&gt;&#xD;
&amp;nbsp;&amp;nbsp; &amp;nbsp;int ar[n];&lt;br /&gt;&#xD;
&amp;nbsp;&amp;nbsp; &amp;nbsp;cout &amp;lt;&amp;lt; &amp;quot;input array:&amp;quot; &amp;lt;&amp;lt; endl;&lt;br /&gt;&#xD;
&amp;nbsp;&amp;nbsp; &amp;nbsp;for(int i = 0;i&amp;lt;n;i++){&lt;br /&gt;&#xD;
&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp;cin &amp;gt;&amp;gt; ar[i];&lt;br /&gt;&#xD;
&amp;nbsp;&amp;nbsp; &amp;nbsp;}&lt;br /&gt;&#xD;
&amp;nbsp;&amp;nbsp; &amp;nbsp;for(int i = n;i&amp;gt;1;i--){&lt;br /&gt;&#xD;
&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp; &lt;span data-darkreader-inline-color="" style="--darkreader-inline-color:#ff1515; color:#ff0000"&gt;&amp;nbsp;ischanged = false; //&#x9810;&#x8A2D;ischanged&#x70BA;false;&amp;nbsp;&lt;/span&gt;&lt;br /&gt;&#xD;
&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp;for(int j = 0;j&amp;lt;i-1;j++){&lt;br /&gt;&#xD;
&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp;if(ar[j]&amp;gt;ar[j+1]){&lt;br /&gt;&#xD;
&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp;tp = ar[j];&lt;br /&gt;&#xD;
&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp;ar[j] = ar[j+1];&lt;br /&gt;&#xD;
&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp;ar[j+1] = tp;&lt;br /&gt;&#xD;
&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp;&lt;span data-darkreader-inline-color="" style="--darkreader-inline-color:#ff1515; color:#ff0000"&gt;ischanged = true; //&#x5982;&#x679C;&#x6709;&#x9032;&#x884C;&#x5143;&#x7D20;&#x4EA4;&#x63DB;,ischanged&#x8A2D;&#x70BA;true&amp;nbsp;&lt;/span&gt;&lt;br /&gt;&#xD;
&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp;}&lt;br /&gt;&#xD;
&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp;}&lt;br /&gt;&#xD;
&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp;&lt;span data-darkreader-inline-color="" style="--darkreader-inline-color:#ff1515; color:#ff0000"&gt;if(ischanged == false) //&#x5982;&#x679C;ischanged&#x70BA;false,&#x5143;&#x7D20;&#x6C92;&#x6709;&#x4EA4;&#x63DB;,&#x4EE3;&#x8868;&#x9663;&#x5217;&#x5DF2;&#x7D93;&#x6392;&#x5E8F;&#x597D;&#x4E86;,&#x8DF3;&#x51FA;&#x8FF4;&#x5708;&#x3002;&amp;nbsp;&lt;br /&gt;&#xD;
&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp;break;&lt;/span&gt;&lt;br /&gt;&#xD;
&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp;for(int i = 0;i&amp;lt;n;i++){&lt;br /&gt;&#xD;
&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp;cout &amp;lt;&amp;lt; ar[i] &amp;lt;&amp;lt; &amp;quot; &amp;quot;;&lt;br /&gt;&#xD;
&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp;}&lt;br /&gt;&#xD;
&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp;cout &amp;lt;&amp;lt; endl;&lt;br /&gt;&#xD;
&amp;nbsp;&amp;nbsp; &amp;nbsp;}&lt;br /&gt;&#xD;
&amp;nbsp;&amp;nbsp; &amp;nbsp;cout &amp;lt;&amp;lt; endl &amp;lt;&amp;lt; &amp;quot;an array has been sorted: &amp;quot;;&lt;br /&gt;&#xD;
&amp;nbsp;&amp;nbsp; &amp;nbsp;for(int i = 0;i&amp;lt;n;i++){&lt;br /&gt;&#xD;
&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp;cout &amp;lt;&amp;lt; ar[i] &amp;lt;&amp;lt; &amp;quot; &amp;quot;;&lt;br /&gt;&#xD;
&amp;nbsp;&amp;nbsp; &amp;nbsp;}&lt;br /&gt;&#xD;
&amp;nbsp;&amp;nbsp; &amp;nbsp;return 0;&lt;br /&gt;&#xD;
}&lt;br /&gt;&#xD;
&lt;br /&gt;&#xD;
