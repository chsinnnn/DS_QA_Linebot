&lt;p&gt;&lt;strong&gt;&#x52D5;&#x614B;&#x898F;&#x5283;(Dynamic Programming)&lt;/strong&gt;&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;&#x5B9A;&#x7FA9;&#x72C0;&#x614B;&#xFF1A;dp[L][R] &#x70BA;&#x53D6; L&amp;nbsp;&#x500B;&#x5DE6;&#x62EC;&#x865F;&#x8207; R&amp;nbsp;&#x500B;&#x53F3;&#x62EC;&#x865F;&#x6709;&#x5E7E;&#x7A2E;&#x5F62;&#x5F0F;&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;&#x8F49;&#x79FB;&#x5F0F;&#xFF1A;&amp;nbsp;&#x7576;&amp;nbsp;&lt;img alt="L \ge R" src="https://latex.codecogs.com/gif.latex?L%20%5Cge%20R" /&gt;&#xFF0C;&#x53EF;&#x4EE5;&#x53D6;&#x5DE6;&#x62EC;&#x865F;&#x8DDF;&#x53F3;&#x62EC;&#x865F;&#xFF0C;dp[L][R] = dp[L-1][R] + dp[L][R-1]&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;&#x57FA;&#x5E95;&#x72C0;&#x614B;&#xFF1A;dp[L][0] &#x7B54;&#x6848;&#x4E00;&#x5B9A;&#x70BA; 1&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;&#x6700;&#x5F8C;&#x7684;&#x7B54;&#x6848;&#x5C31;&#x662F;dp[N][N]&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;```cpp&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;void solve() {&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;&amp;nbsp; int n;&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;&amp;nbsp; cin &amp;gt;&amp;gt; n;&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;&amp;nbsp; vector&amp;lt;vector&amp;lt;int&amp;gt;&amp;gt; dp(n + 1, vector&amp;lt;int&amp;gt;(n + 1, 0));&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;&amp;nbsp; for (int l = 1; l &amp;lt;= n; l++)&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;&amp;nbsp; &amp;nbsp; dp[l][0] = 1;&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;&amp;nbsp; for (int l = 1; l &amp;lt;= n; l++) {&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;&amp;nbsp; &amp;nbsp; for (int r = 1; r &amp;lt;= l; r++) {&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; dp[l][r] = dp[l - 1][r] + dp[l][r - 1];&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;&amp;nbsp; &amp;nbsp; }&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;&amp;nbsp; }&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;&amp;nbsp; cout &amp;lt;&amp;lt; dp[n][n] &amp;lt;&amp;lt; endl;&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;}&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;```&lt;/p&gt;&#xD;
&#xD;
