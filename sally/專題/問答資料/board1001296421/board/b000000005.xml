  <content>&lt;p&gt;122. &#x5BEB;C++&#x7A0B;&#x5F0F;&#x78BC;&#x8AAA;&#x660E;&#x50B3;&#x53C3;&#x8003;&#x547C;&#x53EB;call by reference&#x3002;&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;#include &amp;lt;stdio.h&amp;gt;&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;void SwitchNum( int &amp;amp; num1, int &amp;amp; num2 ) { // call by reference ( address ) ( &#x82E5;&#x7121; &amp;amp; &#x5373;&#x70BA; call by value&amp;nbsp;)&amp;nbsp;&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;int temp = 0; // &#x66AB;&#x5B58;&#x8B8A;&#x6578;&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;temp = num1 ;&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;num1 = num2 ;&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;num2 = temp ;&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;return ;&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;} &amp;nbsp;&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;int main() {&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;&amp;nbsp; int num1 = 1 ;&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;&amp;nbsp; int num2 = 2;&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;&amp;nbsp; printf( &amp;quot;Before&#xFF1A;\n&amp;quot;);&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;&amp;nbsp; printf( &amp;quot;num1&#xFF1A;%d\n&amp;quot;, num1 );&amp;nbsp;&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;&amp;nbsp; printf( &amp;quot;num2&#xFF1A;%d\n&amp;quot;, num2 );&amp;nbsp;&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;&amp;nbsp; SwitchNum( num1, num2 ); // &#x4EA4;&#x63DB;num1&#x548C;num2&#x7684;&#x6578;&#x5B57;&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;&amp;nbsp; printf( &amp;quot;After&#xFF1A;\n&amp;quot;);&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;&amp;nbsp; printf( &amp;quot;num1&#xFF1A;%d\n&amp;quot;, num1 );&amp;nbsp;&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;&amp;nbsp; printf( &amp;quot;num2&#xFF1A;%d\n&amp;quot;, num2 );&amp;nbsp;&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;&amp;nbsp; return 0;&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;}&amp;nbsp;&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;=======================================&lt;/p&gt;&#xD;
&#xD;
