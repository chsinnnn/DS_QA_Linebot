  <content>&lt;p&gt;&#x53EF;&#x4EE5;&#x901A;&#x904E;&#x6539;&#x8B8A;&#x51FD;&#x6578;&#x53C3;&#x6578;&#x7684;&#x6578;&#x91CF;&#xFF0C;&#x985E;&#x578B;&#x4F86;&#x5BE6;&#x73FE;&#x51FD;&#x6578;&#x591A;&#x8F09;(overloading)&#x3002;&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;&#x4EE5;&#x4E0B;&#x662F;&#x4E00;&#x500B;&#x7C21;&#x55AE;&#x7684;&#x4F8B;&#x5B50;~&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;---&lt;br /&gt;&#xD;
class MyClass {&lt;br /&gt;&#xD;
public:&lt;br /&gt;&#xD;
&amp;nbsp; // Method Overloading (Function Overloading) in C++&lt;br /&gt;&#xD;
&amp;nbsp; void display() {&lt;br /&gt;&#xD;
&amp;nbsp; &amp;nbsp; std::cout &amp;lt;&amp;lt; &amp;quot;Hello&amp;quot;&amp;lt;&amp;lt; std::endl;&lt;br /&gt;&#xD;
&amp;nbsp; }&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;&amp;nbsp; void display(int x) {&lt;br /&gt;&#xD;
&amp;nbsp; &amp;nbsp; std::cout &amp;lt;&amp;lt; &amp;quot;Hello, your number is: &amp;quot;&amp;lt;&amp;lt; x &amp;lt;&amp;lt; std::endl;&lt;br /&gt;&#xD;
&amp;nbsp; }&lt;br /&gt;&#xD;
};&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;int main() {&lt;br /&gt;&#xD;
&amp;nbsp; MyClass obj;&lt;/p&gt;&#xD;
&#xD;
&lt;p&gt;&amp;nbsp; // Call without parameters&lt;br /&gt;&#xD;
&amp;nbsp; obj.display();&lt;br /&gt;&#xD;
&amp;nbsp;&amp;nbsp;&lt;br /&gt;&#xD;
&amp;nbsp; // Call with one integer parameter&lt;br /&gt;&#xD;
&amp;nbsp; obj.display(5);&lt;br /&gt;&#xD;
&amp;nbsp;&amp;nbsp;&lt;br /&gt;&#xD;
&amp;nbsp; return 0;&lt;br /&gt;&#xD;
}&lt;br /&gt;&#xD;
---&lt;/p&gt;&#xD;
&#xD;
